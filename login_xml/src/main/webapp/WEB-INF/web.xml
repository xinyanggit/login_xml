<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <display-name></display-name>	
  <servlet>
    <servlet-name>Login</servlet-name>
    <servlet-class>cn.hncu.login.Login</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>RegDemo</servlet-name>
    <servlet-class>cn.hncu.login.RegDemo</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ImgDemo</servlet-name>
    <servlet-class>cn.hncu.login.Img.ImgDemo</servlet-class>
  </servlet>



  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>

  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RegDemo</servlet-name>
    <url-pattern>/reg</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ImgDemo</servlet-name>
    <url-pattern>/immg</url-pattern>
  </servlet-mapping>
</web-app>
